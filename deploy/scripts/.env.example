# MongoDB Configuration for Air-Gapped Environment
MONGODB_URL=mongodb://username:password@your-mongo-host:27017/vlan_manager?authSource=admin
DATABASE_NAME=vlan_manager

# Site Configuration
SITES=site1,site2,site3

# Optional: MongoDB credentials if using separate variables
MONGO_USERNAME=your_username
MONGO_PASSWORD=your_password
MONGO_HOST=your-mongo-host
MONGO_PORT=27017
MONGO_AUTH_DB=admin